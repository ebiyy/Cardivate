{
  "$schema": "https://json.schemastore.org/package",
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "pnpm exec next-on-pages",
    "preview": "nr pages:build && wrangler pages dev",
    "deploy": "nr pages:build && wrangler pages deploy",
    "clear": "rm -rf .next && rm -rf .vercel && rm -rf .wrangler && rm -rf .turbo"
  },
  "dependencies": {
    "@repo/ui": "workspace:*",
    "next": "^14.2.13",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5",
    "@cloudflare/next-on-pages": "^1.13.3",
    "wrangler": "^3.78.7",
    "tailwindcss": "^3.4.12",
    "postcss": "^8.4.47",
    "autoprefixer": "^10.4.20"
  }
}
